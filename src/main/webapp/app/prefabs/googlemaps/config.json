{
    "group": "location",
    "templateUrl": "/pages/Main/Main.html",
    "controllerUrl": "/pages/Main/Main.js",
    "variablesUrl": "/pages/Main/Main.variables.json",
    "iconUrl": "/resources/images/imagelists/prefab-icon.png",
    "resources": {
        "styles": [
            "/pages/Main/Main.css"
        ],
        "scripts": [
            "https://maps.googleapis.com/maps/api/js?v=3.17&sensor=false&callback=onPrefabResourceLoad"
        ],
        "modules": [
            {
                "name": "uiGmapgoogle-maps",
                "files": [
                    "/resources/js/angular-google-maps.min.js"
                ]
            }
        ]
    },
    "properties": {
        "locations": {
            "bindable": "in-bound",
            "type": "array",
            "widget": "string"
        },
        "lat": {
            "type": "list"
        },
        "lng": {
            "type": "list"
        },
        "icon": {
            "type": "list"
        },
        "info": {
            "type": "list"
        },
        "height": {
            "type": "string",
            "value": "400px"
        },
        "zoom": {
            "type": "string",
            "value": 5
        },
        "refresh": {
            "type": "method",
            "method": "refresh"
        }
    }
}